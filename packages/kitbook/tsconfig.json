{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    // "strict": true,
    "paths": {
      "$lib": [
        "./src/lib"
      ],
      "$lib/*": [
        "./src/lib/*"
      ],
      "kitbook": [
        "./src/lib"
      ],
      "kitbook/*": [
        "./src/lib/*"
      ]
    },
    "resolveJsonModule": true,
    "types": [
      "vitest/globals",
      "vitest/importMeta"
    ],
    "allowJs": true,
    "checkJs": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
